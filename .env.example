# Mercado Pago Access Token
# Use the access token for your Mercado Pago account.
MP_ACCESS_TOKEN="APP_USR-XXXXXXXXXXXXXXXXXX"

# Odoo JSON-RPC Configuration
# The JSON-RPC endpoint will be ODOO_URL/jsonrpc
ODOO_URL="https://your-instance.odoo.com"
ODOO_DB="your_database_name"
ODOO_USER="your_odoo_username"
ODOO_PASS="your_odoo_password"

# Odoo Bank Journal ID
# Numeric ID of the bank journal to use for reconciliation.
BANK_JOURNAL_ID=1

# Server Configuration
# Port on which your Node.js app will listen.
PORT=3000
